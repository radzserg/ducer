{
  "name": "ducer",
  "version": "0.0.1",
  "description": "client agnostic data seeder library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-prod": "rm -Rf ./dist/* && NODE_ENV=production tsc --build tsconfig.json && cp README.md package.json dist/",
    "test": "jest",
    "test:types": "jest -c jest.config.tsd.js",
    "prettify": "prettier --write --ignore-unknown src",
    "pre-commit": "lint-staged",
    "lint": "yarn eslint src/**/**.ts"
  },
  "keywords": [
    "seeder",
    "fixtures"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write --ignore-unknown",
      "eslint"
    ]
  },
  "author": "radzserg@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "latest",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^27.4.7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
